#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
char characters[7];

    cout << "Input numbers and letters: \n";
    for (int i = 0; i < 7; i++)
    {
        cin >> characters[i];
    
    }

    cout << endl;
    cout << "Reverse : " << endl;
    for (int s = 6 ; s >= 0; s--)
    {
        cout  << characters[s] << endl;
      
    }
_getch();
return 0;
}
